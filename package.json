    "babel-preset-react": "^6.24.1",
    "crypto-js": "^3.1.9-1",
    "electron": "^1.7.9",
    "electron-builder": "^19.54.0",
    "react": "^16.0.0",
    "react-bootstrap": "^0.31.5",
    "react-dom": "^16.0.0",
    "scrypt-async": "^1.3.1",
    "webpack": "^3.8.1"
